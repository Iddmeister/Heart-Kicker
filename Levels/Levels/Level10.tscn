[sub_resource type="GDScript" id=1]
script/source = "extends Node

func _ready():
	
	if Manager.firstTry:
	
		var speech = get_parent().get_node(\"Objects/Speech\")
		speech.displayText(\"This Heart is for somebody very important\", 0.05)
		yield(speech, \"done\")
		yield(get_tree().create_timer(1), \"timeout\")
		speech.displayText(\"And some people don't want them to get it\", 0.05)
		yield(speech, \"done\")
		yield(get_tree().create_timer(1), \"timeout\")
		speech.displayText(\"They will need to be dealt with\", 0.05)
		yield(speech, \"done\")
		yield(get_tree().create_timer(1), \"timeout\")
		get_parent().get_node(\"Objects/HeartDispenser\").open()
		
	else:
		get_parent().get_node(\"Objects/HeartDispenser\").open()
	pass
"

